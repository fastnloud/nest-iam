  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/cqrs": "^10.0.0 || ^11.0.0",
    "@nestjs/jwt": "^10.0.0 || ^11.0.0",
    "@nestjs/swagger": "^8.0.0 || ^11.0.6",
    "express": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.9",
    "npm-check-updates": "^17.1.15",
    "prettier": "^3.5.3",
